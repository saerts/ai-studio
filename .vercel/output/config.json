{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/ai-voor-bedrijven/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/blog/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/diensten/?$",
			"dest": "_render"
		},
		{
			"src": "^/nieuws/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/nieuws/?$",
			"dest": "_render"
		},
		{
			"src": "^/privacy/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/terms/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}